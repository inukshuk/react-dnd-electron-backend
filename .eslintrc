---
root: true
extends:
  - 'eslint:recommended'
parserOptions:
  ecmaVersion: 2018
  sourceType: module
  ecmaFeatures:
    modules: false
    jsx: true
env:
  es6: true
  node: true
  browser: true
rules:
  class-methods-use-this: 0
  no-confusing-arrow: 0
  no-param-reassign: 0

